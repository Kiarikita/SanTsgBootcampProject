@model IEnumerable<HotelSearch.Web.Models.Response.HotelProduct.GetArrivalAutocompleteResponse.Item>

 
<table class="mt-4 table table-hover table-bordered table-sm">
    <thead>
        <tr> 
            <td>Şehir</td>
            <td>Otel</td> 
            <td>Listele</td> 
             
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.city.name</td> 
                <td>@item.hotel.name</td>
                <td class="align-middle">
                     <a class="btn btn-outline-primary" asp-action="ProductInfoList"  onclick="@("window.location.href='" + @Url.Action("ProductInfoList", "HotelController") + "'");" ><i class="bi bi-pencil-square"></i>Listele </a>
                </td>
            </tr>
         
        }
       
    </tbody>
</table>
